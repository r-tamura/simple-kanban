{
  "name": "simple-kanban",
  "version": "0.0.1",
  "description": "Simple Application",
  "main": "public/index.js",
  "scripts": {
    "watch": "npm run build -- --watch",
    "start:hot": "node server.js",
    "lint": "eslint --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty src *.js",
    "hot-server": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.development.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./src/client/main/index.js",
    "dev": "cross-env START_HOT=1 cross-env PORT=8080 npm run hot-server",
    "test": "jest --coverage",
    "test:update": "npm test -- -u",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/rtamura30/simple-kanban.git"
  },
  "keywords": [
    "Kanban",
    "react",
    "redux",
    "redux-observable",
    "babel",
    "webpack",
    "postcss"
  ],
  "author": "rtamura",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/rtamura30/simple-kanban/issues"
  },
  "homepage": "https://gitlab.com/rtamura30/simple-kanban#README",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.27.3",
    "devtron": "^1.4.0",
    "electron": "^1.6.2",
    "electron-devtools-installer": "^2.1.0",
    "enzyme": "^2.5.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.42.0",
    "html-webpack-plugin": "^2.24.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^16.0.2",
    "postcss-apply": "^0.6.1",
    "postcss-cssnext": "^2.9.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.1",
    "postcss-nesting": "^2.3.1",
    "postcss-sorting": "^2.0.1",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.3.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "spectron": "^3.4.0",
    "style-loader": "^0.16.0",
    "stylelint": "^7.7.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "electron-debug": "^1.1.0",
    "material-ui": "^0.17.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "reselect": "^3.0.0"
  },
  "jest": {
    "moduleDirectories": [
      "src/client",
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/node_moules/",
      "/webpack/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
